{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    //MinimumLevel options are Verbose, Debug, Information, Warning, Error, Fatal
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  }
}