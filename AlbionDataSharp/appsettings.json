{
  "Network": {
    //these are used by AlbionData deduper and should not be used
    "MarketOrdersIngestSubject": "marketorders.ingest",
    "MarketHistoriesIngestSubject": "markethistories.ingest",
    "GoldDataIngestSubject": "goldprices.ingest",

    //AlbionData East and West Servers. Uses POW
    "AlbionDataServers": {
      "East": {
        "url": "http://pow.east.albion-online-data.com",
        "name": "Albion Data East"
      },
      "West": {
        "url": "http://pow.west.albion-online-data.com",
        "name": "Albion Data West"
      }
    },

    //Can use multiple servers. Private doesn't use POW
    "PrivateWestServers": [
      {
        "url": "nats://localhost:4222",
        "name": "Local West Server"
      }
    ],
    "PrivateEastServers": [
    ],
    "ThreadLimitPercentage": 0.25 //decimal percentage of threads to use for each PoW calculation
  },
  "UIConfig": {
    "TitleLine": 1,
    "ServerLine": 4,
    "NameLine": 3,
    "LocationLine": 5,
    "OffersLine": 7,
    "RequestsLine": 8,
    "HistoriesMonthLine": 9,
    "HistoriesWeekLine": 10,
    "HistoriesDayLine": 11,
    "StateStartLine": 13,
    "StateLineCount": 5
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    //MinimumLevel options are Verbose, Debug, Information, Warning, Error, Fatal
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": { "path": "Logs/log.txt" }
      }
    ]
  }
}